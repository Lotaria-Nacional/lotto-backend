{
  "name": "lotto-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "dev": "cross-env NODE_ENV=development tsx --watch src/index.ts",
    "prisma:dev": "dotenv -e .env.development -- prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:prod": "dotenv -e .env.production -- prisma migrate deploy --schema=prisma/schema.prisma",
    "prisma:generate": "prisma generate --env-file .env.production",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "seed:dev": "dotenv -e .env.development -- tsx src/database/index.ts",
    "seed:prod": "dotenv -e .env.production -- tsx src/database/index.ts",
    "build": "tsc",
    "test:e2e": "vitest run"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@imagekit/nodejs": "^7.0.1",
    "@lotaria-nacional/lotto": "^1.0.90",
    "@lotaria-nacional/website-contracts": "^1.0.6",
    "@prisma/client": "^6.16.1",
    "@types/cookie-parser": "^1.4.9",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "papaparse": "^5.5.3",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.15",
    "@types/papaparse": "^5.3.16",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.16.1",
    "supertest": "^7.1.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
